{
  "main": "public/electron_starter.js",
  "homepage": "./",
  "name": "robot_react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "electron-is-dev": "^1.1.0",
    "nipplejs": "^0.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-key-handler": "^1.2.0-beta.3",
    "react-nipple": "^1.0.1",
    "react-scripts": "2.1.8",
    "socket.io": "^2.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "build -c.extraMetadata.main=build/electron_starter.js"
    
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "typescript": "^3.4.5",
    "wait-on": "^3.2.0"
  },
  "author": {
    "name": "Mateusz Gołębiowski",
    "email": "golebiowski.mateusz@outlook.com",
    "url": "https://github.com/mateusz-golebiowski/"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories":{
      "buildResources": "assets"
    }
  }
}
